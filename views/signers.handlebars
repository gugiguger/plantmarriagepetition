<div id="signers">

    <p id="signersmessage">WE JUST SIGNED AND ITS COOL</p>
<a href="/thankyou">Back</a>
<p>{{message}}</p>
<ul>
    {{#each signaturelist}}
    {{#if url}}

    <p>
        <a href="{{url}}">
        <span class="names">{{firstname}} {{lastname}}</span></a>
        <span class="age">{{age}}</span>
        <a href="/signers/{{city}}">
        <span class="city">{{ city}}</span></a>
    </p>

    {{else}}

    <p>
        <span class="names">{{firstname}} {{lastname}}</span>
        <span class="age">{{age}}</span>
        <a href="/signers/{{city}}">{{city}}</a>
    </p>

    {{/if}}
    {{/each}}
</ul>
</div>
